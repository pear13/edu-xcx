/* pages/company/company.wxss */
.m-aboutMeBox{
  width: 100%;
  height: auto;
   display: flex;
  flex-direction: column;
  align-items: center;
}
.m-aboutMe-logo{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
}

.introduce {
  width: 350rpx;
  padding: 100rpx 0 0 20rpx;
}

.top {
  width: 100%;
  height: 227rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  text-align: center;
  position: relative;
  justify-content: center;
}

.logo {
  width: 114rpx;
  height: 114rpx;
  border-radius: 100px;
  border: solid 1px #e4e4e4;
}
.middle{
  width: 713rpx;
	min-height: 540rpx;
  position: relative;
  display: flex;
  flex-direction: column;
  align-items: center;
  border: 15px solid transparent;
  border-image:url('http://vie.zhengqi1995.com/jianjie.png') 30 30 round;
  padding-bottom: 30rpx;
  box-sizing: border-box;
  background-color:white; 
}
.jianjiebg{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: -1;
}
.title{
  width: 100%;
  height: 40px;
  text-align: center;
  font-family: PingFang-SC-Regular;
	font-size: 16px;
	font-weight: normal;
	font-stretch: normal;
	line-height: 40px;
	letter-spacing: 0px;
	color: #333333;
  z-index: 1;
  margin-top: 20rpx;
}
.content{
  width: 587rpx;
  height: auto;
  font-family: PingFang-SC-Regular;
	font-size: 14px;
	font-weight: normal;
	font-stretch: normal;
	line-height: 23px;
	letter-spacing: 0px;
	color: #555555;
  margin-top: 20rpx;
}