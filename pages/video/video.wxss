/* pages/video/video.wxss */

.wrapper {
    width: 100%;
    height: 400rpx;
    position: relative;
}

.video-video {
    width: 100%;
    height: 400rpx;
}

.video-video2 {
    width: 690rpx;
    height: 400rpx;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 99;
}
.videoPlay{
	position: absolute;
    width: 60rpx;
    height: 60rpx;
    top: 50%;
    left: 50%;
    margin: -30rpx 0 0 -30rpx;
	z-index: 100;
}