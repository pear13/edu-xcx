    height: 22rpx;
}

.time {
    font-size: 26rpx;
    font-weight: normal;
    font-stretch: normal;
    line-height: 50rpx;
    letter-spacing: 0px;
    color: #777;
}

.label-icon {
    width: 14rpx;
    line-height: 50rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    margin-right: 17rpx;
}

.label-icon image {
    width: 14rpx;
    height: 20rpx;
}

