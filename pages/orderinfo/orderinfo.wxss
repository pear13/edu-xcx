@import "../teamdetail/teamdetail.wxss";
.stu{
    max-width: 100%;
    max-height: 100%;
}
.stu-detail  {
    width: 80%;
}
.stu-detail image {
    width: 100%;
}
.sublist {
    display: flex;
    flex-direction: row;
    align-items: center;
    padding-bottom: 25rpx;
    background: #fff;
    padding: 30rpx;
}

.right {
    width: 205rpx;
    height: 153rpx;
    border-radius: 10rpx;
    margin-right: 20rpx;
}

.right image {
    width: 205rpx;
    height: 153rpx;
    border-radius: 10rpx;
}

.left {
    display: flex;
    flex-direction: column;
    align-items: flex-start;
    justify-content: flex-start;
    width: 100%;
}

.title {
    max-width: 480rpx;
    font-size: 32rpx;
    font-weight: normal;
    font-stretch: normal;
    line-height: 50rpx;
    letter-spacing: 0px;
    color: #333;
}

.flex-time {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: center;
}

.time-icon {
